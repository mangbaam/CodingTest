   "source": [
    "def solution(n):\n",
    "    answer = 0\n",
    "    li = []\n",
    "    i = 1\n",
    "    while i <= n:\n",
    "        if not n % i:\n",
    "            li.append(i)\n",
    "        i += 1\n",
    "    return sum(set(li))"
   ]
  }
