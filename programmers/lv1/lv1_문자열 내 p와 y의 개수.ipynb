    "def solution(s):\n",
    "    s = s.lower()\n",
    "    pnum, ynum = 0, 0\n",
    "    for c in s:\n",
    "        if c == 'p':\n",
    "            pnum += 1\n",
    "        if c == 'y':\n",
    "            ynum += 1\n",
    "\n",
    "    return True if pnum == ynum else False"
   ]
  }
