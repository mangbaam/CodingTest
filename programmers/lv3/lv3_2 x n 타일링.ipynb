   "source": [
    "def solution(n):\n",
    "    d = [0] * max(n+1, 2)\n",
    "    d[0], d[1] = 1, 1\n",
    "    for i in range(2, n+1):\n",
    "        d[i] = (d[i-2]+d[i-1]) % 1000000007\n",
    "    return d[n]"
   ]
  },
