    "from math import gcd\n",
    "\n",
    "def solution(arr):\n",
    "    for i in range(1, len(arr)):\n",
    "        arr[i] = arr[i-1] * arr[i] // gcd(arr[i-1], arr[i])\n",
    "    return arr[-1]"
   ]
