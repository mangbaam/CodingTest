    "gyu = list(map(int, input().split()))\n",
    "gyu_cal = [15, 28, 19]\n",
    "\n",
    "while not gyu[0]==gyu[1]==gyu[2]:\n",
    "    max_num = max(gyu)\n",
    "    for i in range(3):\n",
    "        if gyu[i] < max_num: gyu[i] += gyu_cal[i]\n",
    "\n",
    "print(gyu[0])"
