   "source": [
    "n = int(input())\n",
    "length = list(map(int, input().split()))\n",
    "oil = list(map(int, input().split()))\n",
    "cheap = oil[0]\n",
    "cost = 0\n",
    "for i in range(n - 1):\n",
    "    cost += length[i] * cheap\n",
    "    cheap = min(cheap, oil[i + 1])\n",
    "print(cost)"
