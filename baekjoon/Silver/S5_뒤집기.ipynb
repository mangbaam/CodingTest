    "num = input()\n",
    "answer = [0, 0]\n",
    "answer[int(num[0])] += 1\n",
    "for i in range(1, len(num)):\n",
    "    if num[i-1] != num[i]:\n",
    "        if num[i] == '0': answer[0] += 1\n",
    "        else: answer[1] += 1\n",
    "print(min(answer))"
