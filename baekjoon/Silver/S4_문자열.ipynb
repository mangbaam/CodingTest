   "source": [
    "a, b = input().split()\n",
    "al, bl = len(a), len(b)\n",
    "answer = 10e9\n",
    "\n",
    "for i in range(bl - al + 1):\n",
    "    count = 0\n",
    "    for j in range(al):\n",
    "        if a[j]!=b[i+j]: count += 1\n",
    "    answer = min(answer, count)\n",
    "print(answer) if answer != 10e9 else print(1)"
