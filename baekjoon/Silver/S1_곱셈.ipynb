    "def rec_fun(n):\n",
    "    if n == 1: return a % c\n",
    "    return (rec_fun(n // 2) ** 2) % c if not n % 2 else (rec_fun(n // 2) ** 2 * a) % c\n",
    "\n",
    "a, b, c = map(int, input().split())\n",
    "print(rec_fun(b))"
   ],
