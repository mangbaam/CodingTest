   "source": [
    "L = int(input())\n",
    "S = [0] + list(sorted(map(int, input().split())))\n",
    "N = int(input())\n",
    "answer = 0\n",
    "\n",
    "for i in range(L):\n",
    "    if N not in range(S[i]+1, S[i+1]): continue\n",
    "    answer = (N-S[i]) * (S[i+1]-N) - 1\n",
    "    break\n",
    "print(answer)"
   ]
  }
