    "n, m = map(int, input().split())\n",
    "ja, mo = 1, 1\n",
    "for i in range(1, m):\n",
    "    ja *= (n-i)\n",
    "    mo *= i\n",
    "ja *= n\n",
    "mo *= m\n",
    "print(ja//mo)"
