   "source": [
    "from collections import deque\n",
    "n = int(input())\n",
    "cards = deque(list(range(1, n+1)))\n",
    "while len(cards) != 1:\n",
    "    cards.popleft()\n",
    "    cards.append(cards.popleft())\n",
    "print(cards[0])"
   ]
  }
