   "source": [
    "while True:\n",
    "    s = input()\n",
    "    if s == \"#\": break\n",
    "    total = 0\n",
    "    for i, c in enumerate(s):\n",
    "        if c == ' ': continue\n",
    "        num = ord(c) - ord('A') + 1\n",
    "        total += num * (i + 1)\n",
    "    print(total)"
   ]
