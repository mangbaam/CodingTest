    "s = input()\n",
    "joi, ioi = 0, 0\n",
    "for i in range(len(s)-2):\n",
    "    sub = s[i]+s[i+1]+s[i+2] \n",
    "    if sub == \"JOI\":\n",
    "        joi += 1\n",
    "    elif sub == 'IOI':\n",
    "        ioi += 1\n",
    "print(joi)\n",
    "print(ioi)"
