    "x = int(input())\n",
    "d = [0] * 1000001\n",
    "\n",
    "for i in range(2, x+1):\n",
    "    li = [d[i-1]]\n",
    "    if i % 3 == 0:\n",
    "        li.append(d[i//3])\n",
    "    if i % 2 == 0:\n",
    "        li.append(d[i//2])\n",
    "    d[i] = min(li) + 1\n",
    "print(d[x])"
