   "source": [
    "n = int(input())\n",
    "a, b = [], []\n",
    "a = list(map(int, input().split()))\n",
    "b = list(map(int, input().split()))\n",
    "a = sorted(a)\n",
    "b = sorted(b, reverse=True)\n",
    "answer = 0\n",
    "for i in range(n):\n",
    "    answer += (a[i] * b[i])\n",
    "print(answer)"
   ]
