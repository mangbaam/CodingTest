   "source": [
    "n = int(input())\n",
    "tiles = [1, 1, 2, 3]+[0]*n\n",
    "\n",
    "for i in range(4, n):\n",
    "    tiles[i] = tiles[i-4]+tiles[i-3]+tiles[i-1]\n",
    "\n",
    "# 한 변의 길이: tiles[n-1], 긴 변의 추가 길이: tiles[n-2]\n",
    "print(4*tiles[n-1] + 2*tiles[n-2])"
   ],
