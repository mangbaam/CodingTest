   "source": [
    "nums = list(range(10))\n",
    "i = 1\n",
    "\n",
    "while i < len(nums):\n",
    "    for n in range(nums[i]%10-1, -1, -1):\n",
    "        nums.append(nums[i]*10+n)\n",
    "    i += 1\n",
    "\n",
    "nums.sort()\n",
    "\n",
    "try:\n",
    "    print(nums[int(input()) - 1])\n",
    "except:\n",
    "    print(-1)"
